msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockExpires"
msgstr "Número de segundos para esquecer as tentativas de login incorretas."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockSeconds"
msgstr "Número de segundos para bloquear uma conta após tentativas incorretas excessivas de login."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockRequired"
msgstr "Se qualquer configuração temporária de bloqueio de conta for inserida, todas as configurações temporárias de bloqueio de conta serão necessárias."

