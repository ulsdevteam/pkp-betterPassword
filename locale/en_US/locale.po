msgid "plugins.generic.betterPassword.displayName"
msgstr "Better Password Plugin"

msgid "plugins.generic.betterPassword.description"
msgstr "Enable additional password requirements within PKP software.  Allows for implementation of NIST recommendations."

msgid "plugins.generic.betterPassword.validation.betterPasswordCheckAlpha"
msgstr "Your password requires an alphabetic character."

msgid "plugins.generic.betterPassword.validation.betterPasswordCheckUppercase"
msgstr "Your password requires an uppercase character."

msgid "plugins.generic.betterPassword.validation.betterPasswordCheckLowercase"
msgstr "Your password requires an lowercase character."

msgid "plugins.generic.betterPassword.validation.betterPasswordCheckNumber"
msgstr "Your password requires an numeric character."

msgid "plugins.generic.betterPassword.validation.betterPasswordCheckSpecial"
msgstr "Your password requires an non-alphanumeric character."

msgid "plugins.generic.betterPassword.validation.betterPasswordCheckBlocklist"
msgstr "Your password was found in a blocklist of known bad passwords."

msgid "plugins.generic.betterPassword.validation.betterPasswordLocked"
msgstr "Your account has been temporarily locked due to bad password attempts."

msgid "plugins.generic.betterPassword.validation.betterPasswordUnexpectedError"
msgstr "Your password could not be changed at this time.  Please try again later."

msgid "plugins.generic.betterPassword.validation.betterPasswordPasswordReused"
msgstr "You cannot reuse this password, please choose another one."

msgid "plugins.generic.betterPassword.manager.settings"
msgstr "Settings"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordUploadFail"
msgstr "Failed to upload file"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordDeleteFail"
msgstr "Failed to delete file"

msgid "plugins.generic.betterPassword.manager.betterPasswordSettings"
msgstr "Better Password Settings"

msgid "plugins.generic.betterPassword.manager.settings.description"
msgstr "<p>With this plugin enabled, user passwords will be checked against additional criteria to ensure better password selection.  NIST recommends checking passwords against a blocklist and using temporary account locking after repeated password failures.  Complexity requirements including specific types of alphanumeric characters are available but not recommended for UX reasons.</p>"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckTitle"
msgstr "Password requirements"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordBlocklist"
msgstr "Upload the blocklist file"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordExistingBlocklist"
msgstr "Uploaded Blocklists"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckAlpha"
msgstr "Require an alphabetic character."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckUppercase"
msgstr "Require an uppercase character."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckLowercase"
msgstr "Require a lowercase character."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckNumber"
msgstr "Require a numeric character."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckSpecial"
msgstr "Require a non-alphanumeric character."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordCheckBlacklist"
msgstr "Check any user-requested passwords against a blocklist of known bad passwords."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockTitle"
msgstr "Temporary account locking"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockTries"
msgstr "Number of bad login attempts after which to lock an account."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockExpires"
msgstr "Number of seconds before forgetting bad login attempts."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockSeconds"
msgstr "Number of seconds to lock an account after excessive bad login attempts."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockRequired"
msgstr "If any temporary account locking setting is entered, all temporary account locking settings are required."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockTriesNumberRequired"
msgstr "Number of bad login attempts after which to lock an account must be numeric."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockExpiresNumberRequired"
msgstr "Number of seconds before forgetting bad login attempts must be numeric."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordLockSecondsNumberRequired"
msgstr "Number of seconds to lock an account after excessive bad login attempts must be numeric."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationTitle"
msgstr "Password invalidation"

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationDays"
msgstr "Number of days before passwords expire."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationMininumWarningDays"
msgstr "Number of days, before the expiration, to start warning the user daily about the password expiration."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationPasswords"
msgstr "Number of last passwords that should be blocked for reuse."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationDaysNumberRequired"
msgstr "Number of days before passwords expire must be numeric."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationPasswordsNumberRequired"
msgstr "Number of last passwords that should be blocked for reuse must be numeric."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationMininumWarningDays"
msgstr "Number of last passwords that should be blocked for reuse must be numeric."

msgid "plugins.generic.betterPassword.manager.settings.betterPasswordInvalidationMininumWarningDays"
msgstr "Number of days, before the expiration, to start warning the user daily about the password expiration must be numeric."

msgid "plugins.generic.betterPassword.message.yourPasswordWillExpire"
msgstr "Your password is going to expire in {$days} day(s)."

msgid "plugins.generic.betterPassword.actions.deleteBlocklist"
msgstr "Delete Existing Blocklist"

msgid "plugins.generic.betterPassword.actions.deleteBlocklistCheck"
msgstr "Are you sure you want to delete?"
